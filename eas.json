{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "18.18.0",
      "env": {
        "EAS_NPM_DISABLE_CI": "1"
      },
      "npm": {
        "command": "install --legacy-peer-deps"
      }
    },
    "preview": {
      "distribution": "internal",
      "node": "18.18.0",
      "env": {
        "EAS_NPM_DISABLE_CI": "1"
      },
      "npm": {
        "command": "install --legacy-peer-deps"
      }
    },
    "production": {
      "autoIncrement": true,
      "node": "18.18.0",
      "env": {
        "EAS_NPM_DISABLE_CI": "1"
      },
      "npm": {
        "command": "install --legacy-peer-deps"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
